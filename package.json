{
  "name": "Cookbook",
  "version": "1.0.0",
  "description": "Cookbook",
  "scripts": {
    "dev": "npm run clean-dev && webpack --config=webpack.config.js",
    "watch": "npm run clean-dev && webpack --config=webpack.config.js --watch --progress",
    "build": "npm run clean-build && webpack --config=webpack.config.prod.js --progress",
    "lint-fix": "eslint ./lib/index.jsx --fix",
    "test": "mocha",
    "test-watch": "mocha -w",
    "clean-dev": "rimraf ./dist/dev/bundle.* && rimraf ./dist/dev/vendor.*",
    "clean-build": "rimraf ./dist/prod/public/bundle.* && rimraf ./dist/prod/public/vendor.* ",
    "clean-nm": "rimraf ./node_modules",
    "deploy": "npm run build && firebase deploy"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "circular-dependency-plugin": "^2.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mockery": "^2.0.0",
    "postcss-loader": "^1.0.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "webpack": "^1.13.3",
    "webpack-livereload-plugin": "^0.9.0"
  },
  "dependencies": {
    "antd": "^2.9.0",
    "axios": "^0.15.3",
    "clipboard-js": "^0.3.3",
    "d3": "^4.8.0",
    "firebase": "^3.7.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
