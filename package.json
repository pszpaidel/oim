{
  "name": "cookbook",
  "version": "1.0.0",
  "description": "Cookbook",
  "scripts": {
    "build:dev": "webpack --config=webpack.config.js --progress --mode development",
    "build:prod": "webpack --config=webpack.config.prod.js --progress --mode production",
    "build:watch": "webpack --config=webpack.config.js --watch",
    "lint-fix": "eslint ./lib/index.jsx --fix",
    "clean-dev": "rimraf ./dist/dev/bundle.* && rimraf ./dist/dev/vendor.* && rimraf ./dist/dev/stats.html",
    "clean-build": "rimraf ./dist/prod/public/bundle.* && rimraf ./dist/prod/public/vendor.* ",
    "clean-nm": "rimraf ./node_modules",
    "build-deploy": "npm run build && firebase deploy",
    "deploy": "firebase deploy"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.28.11",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.0.15"
  },
  "dependencies": {
    "antd": "^2.10.4",
    "axios": "^0.16.1",
    "classnames": "^2.2.5",
    "clipboard-js": "^0.3.3",
    "d3": "^4.9.1",
    "firebase": "^3.7.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  }
}
